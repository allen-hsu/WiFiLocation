@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>定位精度统计</title>
    <link rel="stylesheet" href="../../Content/bootstrap.css" />
    <style>
        html,body{
            height:100%;
            background:#eee;
        }
        .container .picture{
            height:500px;
            margin-top:20px;
            display:none;
        }
        .top {
            position: fixed;
            top: 0;
            width: 100%;
            height: 60px;
            background: #428bca;
            font-size:25px;
            color:#fff;
            line-height:60px;
            padding:0 10px;
            z-index:999;
        }
        .exit{
            font-size:16px;
            line-height:60px;
            cursor:pointer;
        }
        .exit:hover{
            color:#ddd;
        }
        .main{
            display:flex;
            padding-top:60px;
        }
        .left_nav{
            width:15%;
            height:100%;
            background:#fff;
            padding-top:40px;
            position:fixed;
        }
        .right{
            margin-left:15%;
            width:85%;
            background:#eee;
        }
        .nav-pills > li > a{
            border-radius:0px;
        }
    </style>
</head>
<body>
    <div class="top">
        WIFI室内定位系统后台管理界面
        <span id="exit" class="exit pull-right">退出</span>
    </div>
    <div class="main">
        <div class="left_nav">
            <ul class="nav nav-pills nav-stacked">
                <li class="active"><a href="/Home/LocationShow">定位精度分析</a></li>
                <li><a href="/Home/RoomShow">房间实时监控</a></li>
                <li><a href="/Home/CoordDataShow">步行轨迹分析</a></li>
            </ul>
        </div>
        <div class="right">
            <h1 class="text-center">定位精度统计分析</h1>
            <div class="container">
                <div class="row">
                    <div class="col-md-4">
                        <label for="room">房间：</label>
                        <select name="room" id="room" class="form-control">
                            <option value="2">实验室1</option>
                            <option value="1">科学办公室</option>
                        </select>
                    </div>
                    <div class="col-md-4">
                        <label for="mobile">手机型号</label>
                        <select name="mobile" id="mobile" class="form-control">
                            <option value="3">小米4</option>
                            <option value="2">华为</option>
                            <option value="6">OPPO</option>
                        </select>
                    </div>
                    <div class="col-md-4">
                        <label for="algorithm">定位算法</label>
                        <select name="algorithm" id="algorithm" class="form-control">
                            <option value="0">KNN定位算法</option>
                            <option value="1">模糊集定位算法</option>
                        </select>
                    </div>
                </div>
                <div class="row text-center" style="margin-top:15px;">
                    <button class="btn btn-primary" id="locationShow">定位精度分析</button>
                </div>
                <div class="row">
                    <div class="col-md-10 col-lg-offset-1 picture" id="deviation">

                    </div>
                </div>
                <div class="row">
                    <div class="col-md-10 col-lg-offset-1 picture" id="position">

                    </div>
                </div>
            </div>
        </div>
    </div>
   
    <script src="../../Scripts/jquery-1.10.2.min.js" type="text/javascript"></script>
    <script src="../../Scripts/echarts.common.min.js" type="text/javascript"></script>
    <script src="../../Scripts/location/location.js" type="text/javascript"></script>
    <script>
        $('#exit').click(function () {
            window.location.replace("/Home/Login");
        });
    </script>
</body>
</html>
